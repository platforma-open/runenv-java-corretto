{
  "name": "@platforma-open/milaboratories.runenv-java-corretto.java-21.0.2.13.1",
  "version": "1.0.0",
  "description": "Amazon Corretto 21.0.2.13.1 software package for Platforma Backend",
  "scripts": {
    "cleanup": "rm -rf ./pkg-*.tgz && rm -rf ./dld && rm -rf ./dist/",
    "build": "../scripts/build.sh 21.0.2.13.1",
    "prepublishOnly": "pl-pkg publish packages --all-platforms --force --log-level=debug"
  },
  "files": [
    "dist/"
  ],
  "block-software": {
    "entrypoints": {
      "main": {
        "environment": {
          "artifact": {
            "type": "environment",
            "runtime": "java",
            "registry": "platforma-open",
            "roots": {
              "linux-x64": "./dld/corretto-linux-x64",
              "linux-aarch64": "./dld/corretto-linux-aarch64",
              "macosx-x64": "./dld/corretto-macosx-x64",
              "macosx-aarch64": "./dld/corretto-macosx-aarch64",
              "windows-x64": "./dld/corretto-windows-x64"
            },
            "binDir": "bin"
          }
        }
      }
    }
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@platforma-sdk/package-builder": "catalog:"
  }
}