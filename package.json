{
  "name": "@milaboratory/runenv-java-corretto",
  "version": "0.1.3",
  "description": "Amazon Corretto software package for Platforma Backend",
  "scripts": {
    "build": "true",
    "pkg:build": "rm -rf dld && ./scripts/build.sh 21.0.2.13.1",
    "publish:packages": "./scripts/publish.sh 21.0.2.13.1",
    "publish:descriptors": "pl-pkg publish descriptors",
    "release": "npm run pkg:build && npm run publish:packages && npm run publish:descriptors"
  },
  "files": [
    "dist/"
  ],
  "block-software": {
    "registries": {
      "binary": {
        "milaboratories": { "storageURL": "s3://milab-euce1-prod-pkgs-s3-platforma-registry/pub/?region=eu-central-1" }
      }
    },
    "packages": {
      "21.0.2.13.1": {
          "environment": {
            "type": "java",
            "registry": { "name": "milaboratories" },
            "entrypointName": "21.0.2.13.1",
            "version": "21.0.2.13.1",
            "roots": {
              "linux-x64": "./dld/corretto-21.0.2.13.1-linux-x64",
              "linux-aarch64": "./dld/corretto-21.0.2.13.1-linux-aarch64",
              "macosx-x64": "./dld/corretto-21.0.2.13.1-macosx-x64",
              "macosx-aarch64": "./dld/corretto-21.0.2.13.1-macosx-aarch64",
              "windows-x64": "./dld/corretto-21.0.2.13.1-windows-x64"
            },
            "binDir": "bin"
          }
      }
    }
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@milaboratory/pl-package-builder": "^2.6.4"
  }
}
